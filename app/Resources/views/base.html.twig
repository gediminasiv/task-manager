
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">

        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta name="description" content="Neon Admin Panel" />
        <meta name="author" content="" />

        <title>{% block title %}{% endblock %}</title>

        <link rel="stylesheet" href="/components/jquery-ui/themes/black-tie/jquery-ui.min.css">
        <link rel="stylesheet" href="/assets/css/font-icons/entypo/css/entypo.css">
        <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Noto+Sans:400,700,400italic">
        <link rel="stylesheet" href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css">
        <link rel="stylesheet" href="/components/datatables.net-bs/css/dataTables.bootstrap.min.css">
        <link rel="stylesheet" href="/components/bootstrap-calendar/css/calendar.min.css">
        <script type="text/javascript" src="/assets/js/calendar-lt-LT.js"></script>
        <link rel="stylesheet" href="/assets/css/bootstrap.css">
        <link rel="stylesheet" href="/assets/css/neon-core.css">
        <link rel="stylesheet" href="/assets/css/neon-theme.css">
        <link rel="stylesheet" href="/assets/css/neon-forms.css">
        <link rel="stylesheet" href="/components/select2/select2-bootstrap.css">
        <link rel="stylesheet" href="/assets/css/custom.css">

        <script src="/assets/js/jquery-1.11.0.min.js"></script>

        <link rel="shortcut icon" href="/assets/images/favicon.png">

    </head>
    <body class="{{ app.user is null ? 'page-body login-page login-form-fall' : 'page-body skin-default' }}">
        <script type="text/javascript">
            var baseurl = 'http://localhost/';
        </script>
        {% if app.user is not null %}
        <div class="page-container">
            {% include 'menu/sidebar-menu.html.twig' %}

            <div class="main-content">

            {% include 'menu/top-menu.html.twig' %}
            <hr style="margin-top:0px;" />

        {% endif %}
                {% block content %}{% endblock %}
        {% if app.user is not null %}
            </div>
        </div>
        {% endif %}
        <!-- Bottom Scripts -->
        <script>
            var dataTableLithuanian = {
                "sEmptyTable":      "{{ 'no_data_in_table'|trans }}",
                "sInfo":            "{{ 'showing_records_from_to'|trans }}",
                "sInfoEmpty":       "{{ 'showing_records_from_0_to_0'|trans }}",
                "sInfoFiltered":    "{{ 'filtered_info'|trans }}",
                "sInfoPostFix":     "",
                "sInfoThousands":   " ",
                "sLengthMenu":      "{{ 'show_menu_length'|trans }}",
                "sLoadingRecords":  "{{ 'loading'|trans }}",
                "sProcessing":      "{{ 'processing'|trans }}",
                "sSearch":          "{{ 'search'|trans }}:",
                "sThousands":       " ",
                "sUrl":             "",
                "sZeroRecords":     "{{ 'no_records_found'|trans }}",

                "oPaginate": {
                    "sFirst": "{{ 'first'|trans }}",
                    "sPrevious": "{{ 'previous'|trans }}",
                    "sNext": "{{ 'next'|trans }}",
                    "sLast": "{{ 'last'|trans }}"
                }
            }
        </script>
        <script src="/assets/js/gsap/main-gsap.js"></script>
        <script src="/assets/js/autocomplete.min.js"></script>
        <script src="/assets/js/bootstrap.js"></script>
        <script src="/assets/js/joinable.js"></script>
        <script src="/assets/js/resizeable.js"></script>
        <script src="/components/datatables.net/js/jquery.dataTables.min.js"></script>
        <script src="/components/blueimp-file-upload/js/jquery.fileupload.js"></script>
        <script src="/components/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
        <script src="/components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
        <script src="/components/bootstrap-calendar/js/calendar.min.js"></script>
        <script src="/components/underscore/underscore-min.js"></script>
        <script src="/assets/js/neon-api.js"></script>
        <script src="/assets/js/jquery.validate.min.js"></script>
        <script src="/assets/js/neon-login.js"></script>
        <script src="/assets/js/neon-custom.js"></script>
        <script src="/assets/js/neon-demo.js"></script>
        <script src="{{ asset('bundles/pugxautocompleter/js/autocompleter-jqueryui.js') }}"></script>
        <script src="/assets/js/custom.js"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
